<main>
  <div class="col-md-3">
    <%= render 'books/list', user: @user, book: @book %>
  </div>


  <% @users.each do |user| %>
    <div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <%= image_tag user.get_profile_image(100,100) %>
      <%= user.name %>
      <li>
        <%= link_to 'Show', user_path(user.id) %>
      </li>
    </div>
  <% end %>
</main>
